{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=localhost;Database=uSeoToolkitV11;Integrated Security=true"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "a2967480-be97-494f-aff6-76fe76c70fbf"
      },
      "Unattended": {
        "UpgradeUnattended": true
      }
    }
  },
  "SeoToolkit": {
    "Global": {
      "AutomaticSitemapsInRobotsTxt": false
    },
    "SiteAudit": {
      "MinimumDelayBetweenRequest": 2,
      "AllowMinimumDelayBetweenRequestSetting": false,
      "Checks": {
        "BrokenLinkCheck": {
          "Enabled": true
        },
        "BrokenImageCheck": {
          "Enabled": true
        },
        "MissingDescriptionCheck": {
          "Enabled": true
        },
        "MissingImageAltCheck": {
          "Enabled": true
        },
        "MissingTitleCheck": {
          "Enabled": true
        }
      }
    },
    "Sitemap": {
      "ShowAlternatePages": true,
      "DisabledModules": [],
      "StructureMode": "MultiRoot"
    },
    "ScriptManager": {
      "DisabledModules": []
    },
    "RobotsTxt": {
      "DisabledModules": []
    },
    "Redirects": {
      "DisabledModules": []
    },
    "MetaFields": {
      "DisabledModules": []
    }
  }
}